	"version": "0.2.0",
	"configurations": [
		{
			"type": "firefox",
			"request": "launch",
			"name": "Firefox w/ parcel",
			"url": "http://localhost:1234",
			"webRoot": "${workspaceFolder}",
			"breakOnLoad": true,
			"sourceMapPathOverrides": {
				"../*": "${webRoot}/*"
			}
		}
	]
}
